<!DOCTYPE html>
<html>
<head>
	<title></title>
</head>
<body>
	<%= form_for [@my_project, @my_entry] do |f| %>
	<%= f.label :hours %>
	<%= f.text_field :hours, size: 4 %>
	<br>
	<%= f.label :minutes %>
	<%= f.text_field :minutes, size: 4 %>
	<br>
	<%= f.label :date %>
	<%= f.text_field :date, size: 4 %>
	<br>
	<%= f.label :comments %>
	<%= f.text_field :comments, size: 4 %>
	<br>
	<%= f.submit 'save' %>
	<%= link_to 'cancel', project_time_entries_path(@my_project) %>
	<% end %>
</body>
</html>